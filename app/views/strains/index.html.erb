<p id="notice"><%= notice %></p>

<h1>Strains</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @strains.each do |strain| %>
      <tr>
        <td><%= strain.name %></td>
        <td><%= link_to 'Show', strain %></td>
        <td><%= link_to 'Edit', edit_strain_path(strain) %></td>
        <td><%= link_to 'Destroy', strain, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Strain', new_strain_path %>
<%= link_to 'Wines', wines_path %>
<%= link_to 'Enologists', enologists_path %>
<%= link_to 'Magazines', magazines_path %>
<%= link_to 'Ratings', ratings_path %>
<%= link_to 'Positons', positions_path %>
<%= link_to 'Contracts', contracts_path %>
